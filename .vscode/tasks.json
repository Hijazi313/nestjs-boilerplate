{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Nest App",
      "type": "shell",
      "command": "npm",
      "args": ["run", "build"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Start Nest App",
      "type": "shell",
      "command": "npm",
      "args": ["run", "start:dev"],
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }
  ]
}
